
@{
    ViewBag.Title = "Admin Panel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .square {
        background: #F2F2F2;
        width: 100%;
        height: 100%;
    }

    h1, h2 {
        color: #2a2b6e;
    }

    #p {
        color: blue;
    }
</style>


<div style="min-height:700px;">

    <br />
    <div class="row text-center">
        <h1><strong><u>Dashboard</u></strong></h1>
    </div>
    <br />

    <div class="row m-20">
        <div class="col-lg-4"></div>
        <div class="col-lg-4">
            <div class="square">
                <br /><br />
                <center><p id="p"><strong>Total Cards</strong></p></center>
                <br />
                <center><h2> @ViewBag.ttlCard </h2></center>
            </div>
        </div>
        
    </div>

    
</div>

